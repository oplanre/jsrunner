{
    "name": "lanre/jsrunner",
    "description": "A simple PHP wrapper for running JavaScript code",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lanre Waju",
            "email": "hi@lnear.dev",
            "homepage": "https://lnear.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Lame\\JSRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lame\\JSRunner\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}