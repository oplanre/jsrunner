{
    "name": "lanre/js-runner",
    "description": "A simple PHP wrapper for running JavaScript code",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Lame\\JSRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lame\\JSRunner\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
